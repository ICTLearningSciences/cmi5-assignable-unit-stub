{
  "name": "react-cmi5-context",
  "version": "0.1.0",
  "description": "",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "files": [
    "build",
    "scripts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf node_modules *.tgz",
    "deps:update": "npx ncu -u",
    "build": "rollup -c",
    "format": "prettier --write '**/*.{js,jsx,json}'",
    "license:fix": "license-check-and-add remove -f license-config.json && license-check-and-add add -f license-config.json",
    "pack:latest": "npm run build && npm pack && cp ${npm_package_name}-${npm_package_version}.tgz ${npm_package_name}-latest.tgz",
    "postinstall": "node ./scripts/postinstall.js",
    "postversion": "git push && git push --tags",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:format": "prettier --check '**/*.{js,jsx,json}'",
    "test:license": "license-check-and-add check -f license-config.json",
    "test:types": "tsc --noemit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ICTLearningSciences/react-cmi5-context.git"
  },
  "keywords": [
    "react",
    "javascript",
    "xapi",
    "cmi5"
  ],
  "author": "larrykirschner@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ICTLearningSciences/react-cmi5-context/issues"
  },
  "homepage": "https://github.com/ICTLearningSciences/react-cmi5-context#readme",
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "license-check-and-add": "^3.0.4",
    "npm-check-updates": "^9.0.1",
    "prettier": "^2.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup": "^2.26.11",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-typescript2": "^0.27.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@gradiant/xapi-dsl": "^1.10.0",
    "app-root-path": "^3.0.0",
    "fs-extra": "^9.0.1",
    "postinstall": "^0.7.0",
    "tincanjs": "^0.50.0"
  }
}
