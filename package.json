{
  "name": "cmi5-assignable-unit-stub",
  "private": true,
  "description": "stub app to test cmi5 integration",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "deps:update": "npx ncu -u",
    "develop": "gatsby develop",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "license:fix": "license-check-and-add remove -f license-config.json && license-check-and-add add -f license-config.json",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "test": "echo \"this project using only cypress e2e testing\"",
    "test:license": "license-check-and-add check -f license-config.json",
    "test:audit": "npm audit --audit-level=moderate --production",
    "test:cypress": "npx run cy:run --spec 'cypress/**/*.spec.js'",
    "test:format": "prettier --check '**/*.{js,jsx,json,ts,tsx}'",
    "test:lint": "eslint src --ext ts --ext tsx --ext js --ext jsx",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@xapi/cmi5": "^0.2.0",
    "gatsby": "^2.24.78",
    "gatsby-plugin-manifest": "^2.4.34",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-typescript": "^2.4.22",
    "license-check-and-add": "^3.0.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "use-deep-compare-effect": "^1.4.0"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "cypress": "^5.3.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "gh-pages": "^3.1.0",
    "npm-check-updates": "^9.1.0",
    "prettier": "2.1.2",
    "start-server-and-test": "^1.11.5",
    "typescript": "^4.0.3"
  }
}
